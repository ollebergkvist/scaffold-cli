{
  "name": "scaffold-cli",
  "preferGlobal": true,
  "bin": {
    "scaffold": "./bin/index.js"
  },
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,md,json}": [
      "eslint",
      "prettier:check"
    ]
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "inquirer": "^9.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "typescript": "^4.8.4"
  }
}
